<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ac.hw.services.collabquiz.entities.UserAnsweredQ"
		table="UserAnsweredQ">

		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="userJid" type="string" column="userJid" />

		<many-to-one name="question"
			class="ac.hw.services.collabquiz.entities.Question" />

		<property name="answeredCorrect" column="answeredCorrect" />

	</class>
</hibernate-mapping>
