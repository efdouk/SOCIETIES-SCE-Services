<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ac.hw.services.collabquiz.entities.PendingJoins"
           table="pendingJoins">

		<id name="joinID" type="int" column="join_id">
            <generator class="native"/>
        </id>
       <property name="toUser" type="string" column="to_user" />
        <property name="fromUser" column="from_user" />
        <property name="groupName" column="group_name" />

    </class>
</hibernate-mapping>